{
  "name": "joculdetectivului",
  "version": "1.0.0",
  "type": "module",
  "description": "Folder-game ecommerce & gameplay",
  "repository": {
    "type": "git",
    "url": "https://github.com/jonluca/vite-typescript-ssr-react"
  },
  "author": "Daniel Anca & Mubbasher Yasin",
  "license": "MIT",
  "scripts": {
    "killport": "fuser -k 7456/tcp",
    "restart": "npm run killport && npm run start",
    "build-and-restart": "npm run build && npm run killport && npm run start",
    "dev:server": "tsx server.ts",
    "dev:client": "npm run build:client && vite --config vite.config.ts dev",
    "build": "rimraf dist && tsc -p tsconfig.json && npm run build:client && npm run build:server && npm run copy-files && node fix-extensions.js",
    "build:client": "vite build --outDir dist/client --ssrManifest",
    "build:server": "vite build --ssr src/client/entry-server.tsx --outDir dist/server",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "coverage": "vitest --coverage",
    "typecheck": "tsc --noEmit",
    "serve": "npm run build && cross-env NODE_ENV=production node ./dist/server",
    "serve:local": "vite serve",
    "clean": "rimraf dist/",
    "copy-files": "copyfiles \"public/**/*\" dist && copyfiles -u 2 \"dist/client/**/*\" dist && copyfiles -u 2 \"dist/client/assets/**/*\" dist/public",
    "format": "prettier --write src types",
    "start": "NODE_ENV=production node dist/server.js"
  },
